{
  "name": "flight-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "prestart": "prisma db push && prisma generate",
    "start": "node dist/server.js",
    "postinstall": "npm run build && prisma generate",
    "build:seed": "tsc -p tsconfig.prisma.json",
    "seed": "prisma db seed"
  },

  "prisma": {
    "seed": "node dist-prisma/prisma/seed.js"
  },

  "dependencies": {
    "@prisma/client": "5.18.0",
    "prisma": "5.18.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.6",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^7.2.1",
    "jsonwebtoken": "^9.0.3",
    "openai": "^4.28.4",
    "pg": "^8.12.0"
  },

  "devDependencies": {
  "@types/bcrypt": "^5.0.2",
  "@types/cors": "^2.8.19",
  "@types/express": "^4.17.21",
  "@types/jsonwebtoken": "^9.0.6",
  "@types/node": "^22.7.4",
  "@types/pg": "^8.11.0",
  "ts-node": "^10.9.2",
  "ts-node-dev": "^2.0.0",
  "typescript": "^5.6.3"
}

}
